{
  "manifest_version": 2,

  "name": "TubeSeries v.001",
  "description": "Play music without any extra tabs.",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
  
}